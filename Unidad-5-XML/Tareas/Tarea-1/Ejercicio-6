      <apellido1></apellido1>
      <apellido2></apellido2>
      <id>000000001A</id>

      <sesion pulsaciones="108" idref="000000001A" tipo="running">
        <duracion></duracion>
        <distancia></distancia>
        <localizacion></localizacion>
        <comentario></comentario>
      </sesion>

      <sesion pulsaciones="120" idref="000000001A" tipo="running">
        <duracion></duracion>
        <distancia></distancia>
        <localizacion></localizacion>
        <comentario></comentario>
      </sesion>
    </cliente>

